# Laravel / Omnimart standard ignores

# Composer vendor (Laravel packages)
core/vendor/

# Node modules (if any)
node_modules/

# Laravel storage (keep the directory but skip contents)
core/storage/*
!core/storage/.gitignore

# Environment & IDE files
.env
.env.*
.idea/
.vscode/
*.log

# OS junk
.DS_Store
Thumbs.db

# Git-lfs broken test PDFs from PayPal vendor package
core/vendor/srmklive/paypal/tests/Mocks/samples/*.pdf 